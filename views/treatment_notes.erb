<h3>treatment_notes</h3>

<% treatment_notes.each do |treatment_note| %>

    <a href="/treatment_notes/<%= treatment_note['id'] %>">
        <p>
            <%= treatment_note['patient_id'] %> <br>
            <%= treatment_note['medication_history'] %> <br>
            <%= treatment_note['medical_history']%> <br>
            <img class="image" src="<%= treatment_note['images']%>" alt="">  <br>
            <%= treatment_note['results']%> <br>
        </p>
    </a>
    

<% end %>